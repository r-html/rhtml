{
  "name": "@rhtml/graphql",
  "version": "0.0.26",
  "description": "Reactive HyperText Markup Language",
  "scripts": {
    "start": "npx parcel ./examples/index.html --out-dir build/examples",
    "patch": "npm run build && npm version patch && npm publish --update-readme --access public && npm run delete-dist",
    "delete-dist": "rm -rf dist",
    "clean": "git clean -dxf",
    "lint": "npx eslint . --ext .ts",
    "lint-fix": "npx eslint . --fix --ext .ts",
    "build": "rm -rf dist && tsc"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "prettier": "^1.19.1",
    "ts-jest": "25.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rxdi/rhtml.git"
  },
  "author": "Kristiyan Tachev",
  "license": "MIT",
  "browserslist": [
    "last 1 chrome versions"
  ],
  "dependencies": {
    "@rxdi/core": "^0.7.40",
    "@rxdi/graphql-client": "^0.7.38",
    "@rxdi/lit-html": "^0.7.42",
    "shades": "^2.1.0",
    "rxjs": "^6.5.4",
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "graphql-tag": "^2.10.1"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts"
}