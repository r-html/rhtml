    "rxjs": "^6.5.4",
    "shades": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rxdi/rhtml.git"
  },
  "author": "Kristiyan Tachev",
  "license": "MIT",
  "browserslist": [
    "last 1 chrome versions"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts"
}
